{
  "launchType": "FARGATE",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "{{ cf_output \"PrivateSubnet1\" }}",
        "{{ cf_output \"PrivateSubnet2\" }}"
      ],
      "securityGroups": [
        "{{ cf_output \"ServiceSecurityGroupId\" }}"
      ],
      "assignPublicIp": "DISABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "{{ cf_output \"TargetGroupArn\" }}",
      "containerName": "app",
      "containerPort": 8000
    }
  ],
  "desiredCount": 1,
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100
  }
}
